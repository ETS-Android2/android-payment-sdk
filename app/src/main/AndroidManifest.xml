<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kh.com.ipay88.sdk.demo">

    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleLib">
        <activity
            android:name="kh.com.ipay88.sdk.demo.SettingActivity"
            android:exported="true"
            android:parentActivityName="kh.com.ipay88.sdk.demo.MainActivity" />
        <activity
            android:name="kh.com.ipay88.sdk.demo.HistoryActivity"
            android:exported="true"
            android:parentActivityName="kh.com.ipay88.sdk.demo.MainActivity" />
        <activity
            android:name="kh.com.ipay88.sdk.demo.ResultActivity"
            android:exported="true"
            android:parentActivityName="kh.com.ipay88.sdk.demo.MainActivity" />
        <activity
            android:name="kh.com.ipay88.sdk.demo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="kh.com.ipay88.sdk.ClientAppSecret"
            android:value="@string/ipay88_app_secret" />
    </application>

</manifest>